#include <iostream>
#include <fstream>
#include <boost/property_tree/ptree.hpp>
#include <zjucad/ptree/ptree.h>

using namespace std;

int main(int argc, char *argv[])
{
  boost::property_tree::ptree pt;
  zjucad::read_cmdline(argc, argv, pt);

  cout << "[INFO] done" << endl;
  return 0;
}
